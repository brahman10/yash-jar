<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/container_card_parent"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_one_start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".26" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_one_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".40" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_two_start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".43" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_two_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".57" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_three_start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".60" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_three_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".74" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_four_start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".345" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_four_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".485" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_five_start"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".515" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/gl_col_five_end"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".655" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/card_background_one"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_magic_card_empty"
        android:padding="5dp"
        android:layout_marginTop="40dp"
        app:layout_constraintBottom_toTopOf="@+id/barrier_row_one_reverse"
        app:layout_constraintEnd_toEndOf="@+id/gl_col_one_end"
        app:layout_constraintStart_toStartOf="@+id/gl_col_one_start"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintVertical_chainStyle="packed">

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/ivCardOne"
            style="@style/CommonMagicCardStyle"
            android:background="@drawable/ic_magic_card_5_base"
            android:padding="5dp"
            android:src="@drawable/ic_magic_card_5_design_1"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/card_background_two"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_magic_card_empty"
        android:padding="5dp"
        android:layout_marginTop="40dp"
        app:layout_constraintBottom_toTopOf="@+id/barrier_row_one_reverse"
        app:layout_constraintEnd_toEndOf="@+id/gl_col_two_end"
        app:layout_constraintStart_toStartOf="@+id/gl_col_two_start"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintVertical_chainStyle="packed">

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/ivCardTwo"
            style="@style/CommonMagicCardStyle"
            android:background="@drawable/ic_magic_card_5_base"
            android:padding="5dp"
            android:src="@drawable/ic_magic_card_5_design_2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/card_background_three"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_magic_card_empty"
        android:padding="5dp"
        android:layout_marginTop="40dp"
        app:layout_constraintBottom_toTopOf="@+id/barrier_row_one_reverse"
        app:layout_constraintEnd_toEndOf="@+id/gl_col_three_end"
        app:layout_constraintHorizontal_chainStyle="packed"
        app:layout_constraintStart_toStartOf="@+id/gl_col_three_start"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintVertical_chainStyle="packed">

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/ivCardThree"
            style="@style/CommonMagicCardStyle"
            android:background="@drawable/ic_magic_card_5_base"
            android:padding="5dp"
            android:src="@drawable/ic_magic_card_5_design_3"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:background="@drawable/ic_magic_card_5_base" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier_row_one"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:barrierDirection="bottom"
        app:constraint_referenced_ids="card_background_one,card_background_two,card_background_three" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier_row_one_reverse"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:barrierDirection="top"
        app:constraint_referenced_ids="card_background_four,card_background_five" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/card_background_four"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_magic_card_empty"
        android:padding="5dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/gl_col_four_end"
        app:layout_constraintStart_toStartOf="@+id/gl_col_four_start"
        app:layout_constraintTop_toBottomOf="@+id/barrier_row_one"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintVertical_chainStyle="packed">

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/ivCardFour"
            style="@style/CommonMagicCardStyle"
            android:background="@drawable/ic_magic_card_5_base"
            android:padding="5dp"
            android:src="@drawable/ic_magic_card_5_design_4"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/card_background_five"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/ic_magic_card_empty"
        android:padding="5dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/gl_col_five_end"
        app:layout_constraintStart_toStartOf="@+id/gl_col_five_start"
        app:layout_constraintTop_toBottomOf="@+id/barrier_row_one"
        app:layout_constraintVertical_bias="0.0"
        app:layout_constraintVertical_chainStyle="packed">

        <androidx.appcompat.widget.AppCompatImageButton
            android:id="@+id/ivCardFive"
            style="@style/CommonMagicCardStyle"
            android:background="@drawable/ic_magic_card_5_base"

            android:padding="5dp"
            android:src="@drawable/ic_magic_card_5_design_5"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>