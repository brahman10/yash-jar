<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/health_insurance_navigation"
    app:startDestination="@id/insuranceLandingPage">

    <fragment
        android:id="@+id/insuranceLandingPage"
        android:name="com.jar.health_insurance.impl.ui.landing_page.InsuranceLandingPage"
        android:label="InsuranceLandingPage">
        <deepLink app:uri="android-app://com.jar.app/healthInsurance/LandingPage/{fromScreen}/{clickTime}" />
        <argument
            android:name="fromScreen"
            app:argType="string"
            app:nullable="false" />
        <argument
            android:name="clickTime"
            app:argType="string"
            android:defaultValue="0"
            app:nullable="false" />
        <action
            android:id="@+id/action_insuranceLandingPage_to_benefitsPage"
            app:destination="@id/benefitsPage" />
        <action
            android:id="@+id/action_insuranceLandingPage_to_addDetailsFragment"
            app:destination="@id/addDetailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/benefitsPage"
        android:name="com.jar.health_insurance.impl.ui.benefits_page.BenefitsPage"
        android:label="BenefitsPage">
        <argument
            android:name="insuranceId"
            app:argType="string"
            app:nullable="true" />
    </fragment>
    <fragment
        android:id="@+id/insurancePlanComparison"
        android:name="com.jar.health_insurance.impl.ui.insurance_plan_comparison.InsurancePlanComparison"
        android:label="InsurancePlanComparison">
        <argument
            android:name="providerId"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_insurancePlanComparison_to_selectHealthInsurancePlanScreen"
            app:destination="@id/selectHealthInsurancePlanScreen" />
    </fragment>
    <fragment
        android:id="@+id/selectHealthInsurancePlanScreen"
        android:name="com.jar.health_insurance.impl.ui.select_insurance_plan.SelectHealthInsurancePlanScreen"
        android:label="SelectHealthInsurancePlanScreen">
        <deepLink app:uri="android-app://com.jar.app/healthInsurance/selectHealthInsurancePlanScreen/{orderId}" />
        <action
            android:id="@+id/action_selectHealthInsurancePlanScreen_to_completePaymentFragment"
            app:destination="@id/completePaymentFragment" />
        <action
            android:id="@+id/action_selectHealthInsurancePlanScreen_to_insurancePlanComparison"
            app:destination="@id/insurancePlanComparison" />
        <argument
            android:name="orderId"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_selectHealthInsurancePlanScreen_to_completePaymentFragment2"
            app:destination="@id/completePaymentFragment" />
    </fragment>
    <fragment
        android:id="@+id/addDetailsFragment"
        android:name="com.jar.health_insurance.impl.ui.add_details.AddDetailsFragment"
        android:label="AddDetailsFragment">
        <deepLink app:uri="android-app://com.jar.app/healthInsurance/addDetails" />
        <action
            android:id="@+id/action_addDetailsFragment_to_selectHealthInsurancePlanScreen"
            app:destination="@id/selectHealthInsurancePlanScreen" />
    </fragment>
    <fragment
        android:id="@+id/completePaymentFragment"
        android:name="com.jar.health_insurance.impl.ui.payment_status_page.PaymentStatusFragment"
        android:label="CompletePaymentFragment">
        <deepLink app:uri="android-app://com.jar.app/healthInsurance/paymentStatusPage/{insuranceId}" />

        <argument
            android:name="insuranceId"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_completePaymentFragment_to_selectHealthInsurancePlanScreen"
            app:destination="@id/selectHealthInsurancePlanScreen" />

    </fragment>
    <fragment
        android:id="@+id/manageInsuranceScreen"
        android:name="com.jar.health_insurance.impl.ui.manage_insurance_screen.ManageInsuranceScreen"
        android:label="ManageInsuranceScreen">
        <deepLink app:uri="android-app://com.jar.app/healthInsurance/manageInsurance/{insuranceId}" />
        <argument
            android:name="insuranceId"
            app:argType="string"
            app:nullable="false" />
        <action
            android:id="@+id/action_manageInsuranceScreen_to_benefitsPage"
            app:destination="@id/benefitsPage" />
        <action
            android:id="@+id/action_manageInsuranceScreen_to_addDetailsFragment"
            app:destination="@id/addDetailsFragment" />
        <action
            android:id="@+id/action_manageInsuranceScreen_to_insuranceTransactionDetailsScreen"
            app:destination="@id/insuranceTransactionDetailsScreen" />

    </fragment>
    <fragment
        android:id="@+id/insuranceTransactionDetailsScreen"
        android:name="com.jar.health_insurance.impl.ui.manage_insurance_screen.InsuranceTransactionDetailsScreen"
        android:label="InsuranceTransactionDetailsScreen">
        <argument
            android:name="transactionId"
            app:argType="string"
            app:nullable="false" />
    </fragment>

    <dialog
        android:id="@+id/SelectPlanAbandonScreen"
        android:name="com.jar.health_insurance.impl.ui.select_insurance_plan.SelectPlanAbandonBottomSheet"
        android:label="InsuranceSelectPlanAbandonScreen">

        <deepLink app:uri="android-app://com.jar.app/healthInsurance/selectPlanAbandonScreen/{returnScreenData}" />

        <argument
            android:name="returnScreenData"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>

    </dialog>

    <dialog
        android:id="@+id/paymentBottomSheet"
        android:name="com.jar.health_insurance.impl.ui.HealthInsurancePaymentBottomSheet"
        android:label="PaymentBottomSheet" >

        <deepLink app:uri="android-app://com.jar.app/healthInsurance/paymentBottomSheet/{initiatePaymentResponse}/{insuranceId}/{amount}/{containerId}/{paymentType}/{selectedUpiAppPackageName}"/>

        <argument
            android:name="initiatePaymentResponse"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>

        <argument
            android:name="insuranceId"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>

        <argument
            android:name="paymentType"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>
        <argument
            android:name="amount"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>

        <argument
            android:name="containerId"
            app:argType="integer"
            app:nullable="false"
            android:defaultValue="-1"/>

        <argument
            android:name="selectedUpiAppPackageName"
            app:argType="string"
            app:nullable="true"
            android:defaultValue="@null"/>

    </dialog>

</navigation>