<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape android:shape="rectangle" >
            <gradient
                android:angle="360"
                android:centerColor="#FFFFFF"
                android:endColor="#3E3953"
                android:startColor="#A08CF5"
                android:type="linear" />
            <corners android:radius="@dimen/dimen_10dp" />

        </shape>
    </item>
    <item
        android:bottom="1dp"
        android:left="1dp"
        android:right="1dp"
        android:top="1dp">
        <shape android:shape="rectangle" >
            <corners android:radius="@dimen/dimen_10dp" />
            <solid android:color="#2E2942" />
        </shape>
    </item>

</layer-list>