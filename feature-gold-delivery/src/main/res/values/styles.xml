<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomChipChoice" parent="@style/Widget.MaterialComponents.Chip.Filter">
        <item name="chipBackgroundColor">@color/feature_gold_delivery_background_color_chip_state_list</item>
        <item name="android:textColor">@color/text_color_chip_state_list</item>
        <item name="chipCornerRadius">4dp</item>
    </style>

    <style name="chipText" parent="TextAppearance.MaterialComponents.Chip">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textStyle">normal</item>
    </style>

    <style name="boldChipText" parent="TextAppearance.MaterialComponents.Chip">
        <item name="android:textSize">14sp</item>
        <item name="android:fontFamily">@font/inter_bold</item>
        <item name="android:textStyle">bold</item>
    </style>

</resources>